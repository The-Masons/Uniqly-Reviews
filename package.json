{
  "name": "Uniqly-Reviews",
  "version": "1.0.0",
  "description": "Serves reviews for Uniqly product page.",
  "main": "./server/server.js",
  "author": "Louis Mun",
  "license": "MIT",
  "scripts": {
    "start": "nodemon ./server/server.js",
    "generate": "node database/generate.js",
    "db-setup": "node database/seed.js",
    "app-dev": "webpack -d --watch",
    "server-dev": "node ./server/server.js",
    "test": "jest --coverage",
    "docker": "npm run generate & npm run db-setup && npm run server-dev && webpack -d"
  },
  "jest": {
    "setupFiles": [
      "./test/test-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "jquery": "^3.3.1",
    "json-loader": "^0.5.7",
    "mongoose": "^5.0.6",
    "mongoose-unique-validator": "^2.0.0",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.3",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "react-test-renderer": "^16.3.2",
    "webpack": "^3.11.0"
  }
}
